{
  "name": "fipps/contao-fileinfo-bundle",
  "description": "Contao 4 File Info Bundle",
  "keywords": [
    "Contao",
    "Inherit File Info"
  ],
  "type": "contao-bundle",
  "homepage": "https://gitlab.fipps.de/contao/contao4/fileinfo-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Arne Borchert",
      "homepage": "http://www.fipps.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://gitlab.fipps.de/contao/contao4/fileinfo-bundle/issues"
  },
  "require": {
    "php": "^5.6|^7.0",
    "contao/core-bundle": "~4.4"
  },
  "suggest": {
    "heimrichhannot/contao-filecredits": "Copyright Information"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Fipps\\FileinfoBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Fipps\\FileinfoBundle\\ContaoManager\\Plugin"
  }
}